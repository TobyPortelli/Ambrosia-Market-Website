    "sass-loader": "^12.4.0",
    "type-fest": "^2.8.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  },
  "dependencies": {
    "css-loader": "^6.5.1",
